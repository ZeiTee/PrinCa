<Window x:Class="PrinCa.AddPowermode"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PrinCa"
        xmlns:mah="http://metro.mahapps.com/winfx/xaml/controls"
        xmlns:iconPacks="http://metro.mahapps.com/winfx/xaml/iconpacks"
        mc:Ignorable="d"
        Title="AddPowermode" Height="143" Width="187" ResizeMode="NoResize">
    <Grid>
        <Label Content="Material:" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top"/>
        <Label Content="Wattage:" HorizontalAlignment="Left" Margin="10,41,0,0" VerticalAlignment="Top"/>
        <Button x:Name="btn_SavePowermode" Content="Save" Margin="0,0,10,10" Height="20" VerticalAlignment="Bottom" HorizontalAlignment="Right" Width="42" Click="btn_SavePowermode_Click"/>
        <mah:NumericUpDown x:Name="numUpDn_Wattage" Margin="72,41,10,0" VerticalAlignment="Top" Value="80" ParsingNumberStyle="Currency" TrackMouseWheelWhenMouseOver="True" Minimum="0"/>
        <ComboBox x:Name="cmbx_Material" Margin="72,10,10,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
